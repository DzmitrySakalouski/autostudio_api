{
  "name": "autostudio_api",
  "version": "1.0.0",
  "description": "api for the app",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "webpack --config webpack.config.js --env=dev --mode development",
    "prestart": "npm run build",
    "start": "node ./dist/autostudio_api.app.bundle.js",
    "preprod": "npm test",
    "prod": "webpack --config webpack.config.js --env=prod --mode production",
    "prestart-prod": "npm run prod",
    "start-prod": "node ./build/autostudio_api.app.bundle.min.js",
    "test": "jest --config jest.config.json --forceExit",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "tslint": "tslint"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "express": "^4.17.1",
    "inversify": "^5.0.5",
    "inversify-express-utils": "^6.3.2",
    "pg-promise": "^10.9.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.5.0",
    "sequelize-typescript": "^2.1.0",
    "ts-lint": "^4.5.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.23.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.29",
    "@types/validator": "^13.1.3",
    "webpack-cli": "^4.5.0"
  }
